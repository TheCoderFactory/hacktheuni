<div class="team container">
<h1>Teams</h1>

<div class="form-actions">
  <%= link_to 'New Team', new_team_path, class: 'btn btn-primary' %>
  <%= link_to 'Sign out', destroy_user_session_path, method: :delete, class: "btn btn-destroy" %>
</div>

<div class="">
  
</div>





<table class="table">
    <tr>
      <th>Name</th>
      <th>Picture</th>
      <th>Description</th>
      <th>Video Url</th>
        <th colspan="3"></th>
    </tr>

  <%= content_tag_for(:tr, @teams) do |team| %>
    <td><%= link_to team.name, team %></td>
    <td><%= image_tag(team.picture_url, class: "img-responsive") %></td>
    <td><%= team.description %></td>
    <td><%= team.video_url %></td> 
  <% end %>
</table>



</div>